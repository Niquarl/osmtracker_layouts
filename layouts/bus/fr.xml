<?xml version="1.0" encoding="utf-8"?>
<layouts>
        <layout name="root">
		<row>
			<button type="picture" icon="logo_camera"/>
			<button type="tag" label="Passage piéton" icon="bus_icons/Regul_pieton.png"/>
			<button type="textnote" icon="logo_textnote"/>
		</row>
		<row>
			<button type="page" label="Régulation" targetlayout="Regulation" icon="bus_icons/Regul_stop.png"/>
			<button type="page" label="Etat de la route" targetlayout="Etat" icon="bus_icons/Etat_excellent.png"/>
			<button type="page" label="Vitesse" targetlayout="Vitesse" icon="bus_icons/Vitesse_pa.png"/>
		</row>
		<row>
			<button type="tag" label="Arrêt de Bus" icon="bus_icons/bus_stop-C6.png"/>
			<button type="page" label="BUS" targetlayout="BUS" icon="bus_icons/bus_stop.png"/>
			<button type="page" label="Plus" targetlayout="Plus" icon="bus_icons/amenity_health.png"/>
		</row>
        </layout>
	<layout name="Regulation">
		<row>
			<button type="tag" label="Cédez-le-passage" icon="bus_icons/Regul_cedez.png"/>
			<button type="tag" label="Stop" icon="bus_icons/Regul_stop.png"/>
			<button type="tag" label="Arrêt de Bus" icon="bus_icons/bus_stop-C6.png"/>
		</row>
		<row>
			<button type="tag" label="priority=forward" icon="bus_icons/Regul_pascedez.png"/>
			<button type="tag" label="highway=traffic_signals" icon="bus_icons/Regul_feu.png"/>
			<button type="tag" label="traffic_sign=FR:AB1" icon="bus_icons/Regul_priorité-droite.png"/>
		</row>
		<row>
			<button type="tag" label="Passage piéton" icon="bus_icons/Regul_pieton.png"/>
			<button type="tag" label="Passage surélévé" icon="bus_icons/Regul_pieton-ralentisseur.png"/>
			<button type="tag" label="Ralentisseur" icon="bus_icons/Regul_dosdane.png"/>
		</row>
        </layout>
	<layout name="Etat">
		<row>
			<button type="tag" label="smoothness=excellent" icon="bus_icons/Etat_excellent.png"/>
			<button type="tag" label="smoothness=good" icon="bus_icons/Etat_good.png"/>
			<button type="tag" label="smoothness=intermediate" icon="bus_icons/Etat_intermediate.png"/>
		</row>
		<row>
			<button type="tag" label="smoothness=bad" icon="bus_icons/Etat_bad.png"/>
			<button type="tag" label="smoothness=very_bad" icon="bus_icons/Etat_very_bad.png"/>
		</row>
        </layout>
	<layout name="Vitesse">
		<row>
			<button type="tag" label="Vitesse connu" icon="bus_icons/Vitesse_co.png"/>
			<button type="tag" label="traffic_sign=FR:B14[xx]|direction=*" icon="bus_icons/Vitesse_pa.png"/>
			<button type="tag" label="Vitesse Fin" icon="bus_icons/Vitesse_00.png"/>
		</row>
		<row>
			<button type="tag" label="maxspeed=30" icon="bus_icons/Vitesse_30.png"/>
			<button type="tag" label="maxspeed=50" icon="bus_icons/Vitesse_50.png"/>
			<button type="tag" label="maxspeed=70" icon="bus_icons/Vitesse_70.png"/>
		</row>
		<row>
			<button type="tag" label="maxspeed=80" icon="bus_icons/Vitesse_80.png"/>
			<button type="tag" label="maxspeed=90" icon="bus_icons/Vitesse_90.png"/>
			<button type="tag" label="maxspeed=110" icon="bus_icons/Vitesse_110.png"/>
		</row>
        </layout>
			<layout name="BUS">
		<row>
			<button type="tag" label="Arrêt de Bus" icon="bus_icons/bus_stop-C6.png"/>
			<button type="tag" label="Abri Bus" icon="bus_icons/bus_abri.png"/>
		</row>
		<row>
			<button type="tag" label="Banc" icon="bus_icons/bus_banc.png"/>
			<button type="tag" label="Ombre"/>
			<button type="tag" label="panneau" icon="bus_icons/bus_info.png"/>
		</row>
		<row>
			<button type="tag" label="Passage pieton" icon="bus_icons/Regul_pieton.png"/>
			<button type="tag" label="Ralentisseur" icon="bus_icons/Regul_dosdane.png"/>
			<button type="tag" label="Passage surélévé" icon="bus_icons/Regul_pieton-ralentisseur.png"/>
		</row>
        </layout>
			<layout name="Droite">
		<row>
			<button type="tag" label="Gauche" icon="bus_icons/Arrow_left.png"/>
			<button type="tag" label="Avant" icon="bus_icons/Arrow_back.png"/>
			<button type="tag" label="Droite" icon="bus_icons/Arrow_right.png"/>
		</row>
		<row>
			<button type="tag" label="Cédez-le-passage" icon="bus_icons/Regul_cedez.png"/>
			<button type="tag" label="BUS STOP" icon="bus_icons/bus_stop-C6.png"/>
			<button type="tag" label="Stop" icon="bus_icons/Regul_stop.png"/>
		</row>
		<row>
			<button type="tag" label="Trottoir" icon="bus_icons/Sidewalk_start.png"/>
			<button type="tag" label="Passge piéton" icon="bus_icons/Regul_pieton.png"/>
			<button type="tag" label="Stationnement de rue" icon="bus_icons/Parking.png"/>
		</row>
        </layout>
</layouts>